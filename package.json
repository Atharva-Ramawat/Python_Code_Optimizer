{
    "name": "auto-code-optimizer",
    "displayName": "AI Code Optimizer",
    "description": "Uses a cloud-hosted AI model to refactor and optimize your selected Python code.",
    "version": "1.0.3",
    "publisher": "AtharvaRamawat",
    "engines": {
        "vscode": "^1.82.0"
    },
    "categories": [
        "Programming Languages",
        "Machine Learning",
        "Education"
    ],
    "keywords": [
        "python",
        "ai",
        "optimizer",
        "refactor",
        "code",
        "clean code"
    ],
    "icon": "images/icon.png",
    "homepage": "https://github.com/YOUR_GITHUB_USERNAME/auto-code-optimizer",
    "repository": {
        "type": "git",
        "url": "https://github.com/YOUR_GITHUB_USERNAME/auto-code-optimizer.git"
    },
    "bugs": {
        "url": "https://github.com/YOUR_GITHUB_USERNAME/auto-code-optimizer/issues"
    },
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "auto-code-optimizer.optimize",
                "title": "Optimize Code with AI"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "auto-code-optimizer.optimize",
                    "group": "modification"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.82.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0"
    },
    "dependencies": {
        "axios": "^1.7.2"
    }
}